 <!DOCTYPE html>
<html>

   <%- include('.././layout/head') %>
<body>
  
    <h6>Hello, <%=data%>!<br>Welcome on Board<br><a class="logout" href="/logout">Logout</a></h6>
    <%- include('.././layout/header') %>
 
    <div id='phrasebox'>
      <p><h2><strong>Saved Phrases</strong></h2></p>
      <br>
 
    <ul>
      
       <% phrasegoup.forEach((phr,uid)=>{ %>
     <li> <br>
      <!-- <p class="ph" id="delete-<%=uid%>"> <%=phr %></p> -->
     
      <form>
          <p class="ph" id="delete-<%=uid%>"> <%=phr %></p>
          <br>
          <!-- <input type="hidden" name="_method" value="DELETE" /> -->
      <button class="deletebutton" type="submit" data-id="<%=uid%>">Delete</button>
      <a href="http://www.localhost:3000/phrases">View</a>
    </form>
    </li>

     <% }) %> 
    </ul>
  </div>
    <canvas id='s'>grass background</canvas>
    <script src="/grass.js"></script>
    <script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>
  <script type="text/javascript" src="/app.js"></script>
</body>
   
</html>